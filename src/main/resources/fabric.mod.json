{
	"schemaVersion": 1,
	"id": "chiseled_lib",
	"version": "${version}",
	"name": "Chiseled Lib",
	"description": "chiseled lib is a library i made for Architektum Workshop to help with mod development and also to prevent us from repeatedly recoding the same thing for each project.",
	"authors": [
		"InfinityFarzad",
		"MongoCat"
	],
	"contact": {
		"homepage": "https://modrinth.com/project/chiseled-lib",
		"sources": "https://github.com/InfinityFarzad/Chiseled_Lib"
	},
	"license": "ARR",
	"icon": "assets/chiseled_lib/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"net.akws.chiseled_lib.common.ChiseledLib"
		],
		"fabric-datagen": [
			"net.akws.chiseled_lib.common.datagen.ChiseledLibDataGenerator"
		],
		"client": [
			"net.akws.chiseled_lib.client.ChiseledLibClient"
		],
		"cardinal-components":[
			"net.akws.chiseled_lib.common.component.ChiseledCCARegistries"
		]
	},
	"mixins": [
		"chiseled_lib.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.16.14",
		"minecraft": "~1.21.5",
		"java": ">=21",
		"fabric-api": "*",
		"cardinal-components-base": ">=6.3.1"
	},
	"suggests": {
		"another-mod": "*"
	},
	"custom": {
		"cardinal-components": [
			"chiseled_lib:screenshake_component"
		]
	}
}